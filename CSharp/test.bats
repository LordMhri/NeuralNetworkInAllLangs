    printf 'Lines:\n'
    printf 'lines %s\n' "${lines[@]}" >&2
    printf 'output %s\n' "${output[@]}" >&2
    [ "${lines[0]}" = "accuracy: 85.876 % (1368/1593), avg confidence: 68.060 %" ]
    [ "${lines[1]}" = "accuracy: 91.965 % (1465/1593), avg confidence: 78.090 %" ]
